{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-immutable",
      "tslint-plugin-prettier",
      "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
      "no-null-keyword": true,
      "no-any": true,
      "no-var-keyword": true,
      "typedef": [true, "call-signature"],
      "readonly-keyword": true,
      "readonly-array": true,
      "no-let": true,
      "no-object-mutation": true,
      "no-mixed-interface": true,
      "prettier": true
    },
    "rulesDirectory": []
}
